{
  "id": "sf2",
  "name": "SF2 Player",
  "abbrev": "SF",
  "version": "0.3.10",
  "description": "SoundFont (.sf2) synthesizer using FluidLite",
  "author": "FluidLite by Robin Lobel (port: charlesvestal)",
  "license": "LGPL-2.1",
  "ui": "ui.js",
  "dsp": "dsp.so",
  "api_version": 2,
  "capabilities": {
    "audio_out": true,
    "audio_in": false,
    "midi_in": true,
    "midi_out": true,
    "aftertouch": true,
    "chainable": true,
    "component_type": "sound_generator",
    "ui_hierarchy": {
      "levels": {
        "root": {
          "name": "SF2",
          "params": [
            {
              "key": "preset",
              "label": "Preset",
              "type": "int",
              "min": 0,
              "max_param": "preset_count",
              "default": 0
            },
            {
              "key": "octave_transpose",
              "label": "Octave",
              "type": "int",
              "min": -4,
              "max": 4,
              "default": 0
            },
            {
              "key": "gain",
              "label": "Gain",
              "type": "float",
              "min": 0,
              "max": 2,
              "default": 1.0
            }
          ],
          "knobs": [
            "octave_transpose",
            "gain"
          ]
        }
      }
    }
  },
  "assets": {
    "path": "soundfonts",
    "label": "Soundfonts",
    "extensions": [
      ".sf2"
    ],
    "description": "Upload .sf2 soundfont files. A default General MIDI soundfont is included.",
    "hint": "Find soundfonts at",
    "hint_url": "https://musical-artifacts.com/artifacts?formats=sf2",
    "hint_url_label": "Musical Artifacts"
  },
  "defaults": {
    "soundfont_path": "/data/UserData/move-anything/modules/sound_generators/sf2/instrument.sf2",
    "preset": 0
  }
}
