{
  "id": "sf2",
  "name": "SF2 Synth",
  "abbrev": "SF",
  "version": "0.3.4",
  "description": "SoundFont (.sf2) synthesizer using TinySoundFont",
  "author": "TinySoundFont by schellingb (port: charlesvestal)",
  "license": "MIT",
  "ui": "ui.js",
  "dsp": "dsp.so",
  "api_version": 2,
  "capabilities": {
    "audio_out": true,
    "audio_in": false,
    "midi_in": true,
    "midi_out": true,
    "aftertouch": true,
    "chainable": true,
    "component_type": "sound_generator",
    "chain_params": [
      {
        "key": "preset",
        "name": "Preset",
        "type": "int",
        "min": 0,
        "max_param": "preset_count",
        "default": 0
      },
      {
        "key": "octave_transpose",
        "name": "Octave",
        "type": "int",
        "min": -4,
        "max": 4,
        "default": 0
      },
      {
        "key": "gain",
        "name": "Gain",
        "type": "float",
        "min": 0,
        "max": 2,
        "default": 1.0
      }
    ]
  },
  "defaults": {
    "soundfont_path": "/data/UserData/move-anything/modules/sound_generators/sf2/instrument.sf2",
    "preset": 0
  }
}
